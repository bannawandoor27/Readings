digraph RosterlyPaymentSync {
	node [color=lightblue shape=rectangle style=filled]
	QBO [label="QuickBooks Online"]
	Plaid [label="Plaid API"]
	node [color=lightgreen shape=rectangle style=filled]
	QBOWorker [label="QBO Payment
Sync Worker"]
	PlaidWorker [label="Plaid Payment
Sync Worker"]
	node [color=khaki shape=rectangle style=filled]
	Payment [label="Payment (root)"]
	QboPayment [label=QboPayment]
	PlaidPayment [label=PlaidPayment]
	ManualPayment [label=ManualPayment]
	PaymentLine [label=PaymentLineItem]
	Invoice [label=Invoice]
	CustInv [label=CustomerInvoice]
	VendInv [label=VendorInvoice]
	node [color=orange shape=rectangle style=filled]
	Reconcile [label="Reconciliation Service"]
	UpdStatus [label="Invoice Status Updater"]
	node [color=plum shape=cylinder style=filled]
	DB [label="Postgres
Rosterly DB"]
	edge [style=solid]
	QBO -> QBOWorker
	QBOWorker -> Payment
	Plaid -> PlaidWorker
	PlaidWorker -> Payment
	Payment -> Reconcile
	Reconcile -> PaymentLine
	PaymentLine -> Invoice
	Invoice -> UpdStatus
	edge [style=dashed]
	Payment -> QboPayment [label="1 : 1"]
	Payment -> PlaidPayment [label="1 : 1"]
	Payment -> ManualPayment [label="1 : 1"]
	Payment -> PaymentLine [label="1 : *"]
	PaymentLine -> Invoice [label="* : 1"]
	Invoice -> CustInv [label="is-a"]
	Invoice -> VendInv [label="is-a"]
}
